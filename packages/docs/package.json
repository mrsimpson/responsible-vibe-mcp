{
  "name": "@responsible-vibe/docs",
  "version": "1.0.0",
  "description": "Documentation site for Responsible Vibe MCP",
  "type": "module",
  "scripts": {
    "prebuild": "mkdir -p public/workflows && cp ../../resources/workflows/*.yaml public/workflows/ && node scripts/generate-workflow-manifest.js",
    "dev": "npm run prebuild && vitepress dev",
    "build": "npm run prebuild && vitepress build",
    "preview": "vitepress preview"
  },
  "dependencies": {
    "@responsible-vibe/visualizer": "workspace:*",
    "d3": "^7.8.5",
    "js-yaml": "^4.1.0",
    "pako": "2.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/pako": "2.0.4",
    "typescript": "^5.0.0",
    "vitepress": "^1.6.3"
  }
}
